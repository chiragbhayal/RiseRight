# .env (React Client)

# Backend API base URL (ensure the backend is running on this host/port)
REACT_APP_API_BASE_URL=http://localhost:5000

# Google Maps API key (if you're using map view features)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Any third-party service keys (e.g., for analytics, chat widgets, etc.)
REACT_APP_ANALYTICS_ID=your_analytics_id_here
